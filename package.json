{
  "name": "tmp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "compile": "tsc",
    "build": "tsc && vite build",
    "watch": "tsc --watch",
    "preview": "vite preview",
    "lint": "npx eslint . --color --ext .ts,.tsx",
    "lint-fix": "npx eslint . --ext .ts,.tsx --fix",
    "lint-md": "markdownlint *.md **/*.md .github/*.md --ignore node_modules",
    "setup": "git config core.hooksPath .githooks",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "clean": "./scripts/clean.sh",
    "postclean": "npm install",
    "postinstall": "npm run patch",
    "todo": "leasot -S 'src/**/*'",
    "todo-ci": "leasot -x -S --reporter markdown 'src/**/*' > TODO.md",
    "todo-pr": "leasot -x -S --reporter markdown 'src/**/*' > PR_TODO.md",
    "db-start": "npx supabase start",
    "db-start-env": "./scripts/db-start.sh",
    "db-stop": "npx supabase stop",
    "db-reset": "npx supabase db reset",
    "patch": "patch-package",
    "cli": "npx tsx cli/command.ts run"
  },
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.15.1",
    "@mui/material": "^5.15.5",
    "@remix-run/web-fetch": "^4.4.2",
    "@supabase/supabase-js": "^2.39.3",
    "nuka-carousel": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loading-skeleton": "^3.4.0",
    "react-router-dom": "^6.22.1"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@inquirer/prompts": "^3.3.0",
    "@storybook/addon-links": "^7.6.4",
    "@storybook/addon-themes": "^7.6.17",
    "@storybook/blocks": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-vite": "^7.6.17",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.25",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.18.1",
    "@types/yargs": "^17.0.32",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "babel-plugin-direct-import": "^1.0.0",
    "chalk": "^5.3.0",
    "dotenv": "^16.4.1",
    "eslint": "^8.56.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-storybook": "^0.6.15",
    "jsdom": "^24.0.0",
    "leasot": "^13.3.0",
    "markdownlint-cli": "^0.39.0",
    "patch-package": "^8.0.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.4",
    "storybook": "^7.6.17",
    "storybook-addon-react-router-v6": "^2.0.10",
    "vite": "^4.5.2",
    "supabase": "^1.148.6",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2",
    "tsx": "^4.7.1",
    "vite": "^4.4.12",
    "vitest": "^1.3.1",
    "yargs": "^17.7.2"
  }
}
